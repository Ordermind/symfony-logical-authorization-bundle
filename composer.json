{
    "type": "symfony-bundle",
    "name": "ordermind/logical-authorization-bundle",
    "version": "0.17.8",
    "license": "MIT",
    "description": "Symfony bundle supporting flexible and convenient model, field and route level permissions.",
    "keywords": [
        "authorization"
    ],
    "authors": [
        {
            "name": "Kristofer Tengstr√∂m",
            "email": "ordermind@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ordermind\\LogicalAuthorizationBundle\\": "src"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ordermind\\LogicalAuthorizationBundle\\Test\\": "tests"
        }
    },
    "require": {
        "doctrine/common": "^3.0",
        "symfony/cache": "^5.1",
        "symfony/config": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "symfony/expression-language": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/yaml": "^5.1",
        "ordermind/logical-permissions": "3.0.0-alpha1"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.11",
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.19",
        "symfony/monolog-bundle": "^3.5",
        "symfony/twig-bundle": "^5.1",
        "symfony/browser-kit": "^5.1"
    }
}